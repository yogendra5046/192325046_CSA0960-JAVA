public class ReverseString {
    public static void main(String[] args) {
        String input = "TEMPLE";
        char[] chars = input.toCharArray();
        String reversed = "";
        for (int i = chars.length - 1; i >= 0; i--) {
            reversed += chars[i];
        }
        System.out.println("Reverse String: " + reversed);
    }
}
