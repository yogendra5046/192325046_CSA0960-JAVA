
public class pract {
		    public static void main(String[] args) {
		        String s = "wow";
		        int count = 0;
		        for (int i = 0; i < s.length(); i++) {
		        	if(s.charAt(i)=='w') { 
		            count = count + 1;
		        	}	}
		        System.out.println(count);
		       
		}}
